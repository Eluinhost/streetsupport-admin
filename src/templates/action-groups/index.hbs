---
title: Action Groups - Street Support Admin
description: Street Support - Making it easier to help homeless people in Manchester
jsName: actionGroups
pageName: actionGroups
---

{{#extend "_default"}}
  {{#content "main"}}
    <div class="block block--highlight">
      <div class="container block__inner">
        <header>
          <h1>Action Groups</h1>
        </header>
      </div>
    </div>
    <div class="block block--main-body">
      <div class="container block__inner">
        <div class="block__content">
          <div data-bind="template: { name: 'group', foreach: actionGroups }"></div>
          {{> loading-spinner}}
        </div>
      </div>
    </div>
  {{/content}}
  {{#content "gutter"}}
  {{/content}}
{{/extend}}

<script type="text/html" id="group">
  <article class="stacked-list__item stacked-list__item--action-group">
    <header>
      <h2><a data-bind="attr: { 'href': url, 'title': name }, html: name"></a></h2>
      <p data-bind="text: synopsis"></p>
    </header>
  </article>
</script>
